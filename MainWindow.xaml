<Window x:Class="DiscordExample.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DiscordExample"
        mc:Ignorable="d"
        Title="MainWindow" Height="600" Width="1200">
    <StackPanel>
        <Label Content="Type words."/>
        <TextBox x:Name="txtThing" TextChanged="txtThing_TextChanged" />
        <Label Content="RTB"/>
        <RichTextBox x:Name="RTB" AcceptsReturn="True"/>
    </StackPanel>

</Window>
